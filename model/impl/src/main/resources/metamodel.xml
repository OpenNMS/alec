<meta-model>
    <model-object-def type="Model"/>
    <model-object-def type="Device">
        <parents-ref>
            <parent-ref type="Model"/>
        </parents-ref>
    </model-object-def>
    <model-object-def type="Card">
        <parents-ref>
            <parent-ref type="Device"/>
        </parents-ref>
    </model-object-def>
    <model-object-def type="Port">
        <parents-ref>
            <parent-ref type="Device"/>
            <parent-ref type="Card"/>
        </parents-ref>
    </model-object-def>
    <model-object-def type="Link">
        <parents-ref>
            <parent-ref type="Model"/>
        </parents-ref>
        <peer-refs>
            <peer-ref type="Port"/> <!-- This implies that the link is the owner -->
        </peer-refs>
    </model-object-def>
</meta-model>