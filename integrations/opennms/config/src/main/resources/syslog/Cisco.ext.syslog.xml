        <!-- %PLATFORM-PWRMON-2-MODULE_FAIL_REASON: Power-module 0/PM4/SP fail reason: Module shutdown due to no external stable input power feed present -->
        <ueiMatch>
            <match type="regex" expression="%PLATFORM-PWRMON-2-MODULE_FAIL_REASON\s*: Power-module (\S+) fail reason: (.*)"/>
            <uei>uei.opennms.org/vendor/cisco/syslog/powerModuleFailureReason</uei>
            <parameter-assignment matching-group="1" parameter-name="powerModule"/>
            <parameter-assignment matching-group="2" parameter-name="reason"/>
        </ueiMatch>
        <!-- %PLATFORM-PWRMON-2-MODULE_FAILURE : Power-module 0/PM4/SP failure condition cleared -->
        <ueiMatch>
            <match type="regex" expression="%PLATFORM-PWRMON-2-MODULE_FAILURE\s*: Power-module (\S+) failure condition cleared"/>
            <uei>uei.opennms.org/vendor/cisco/syslog/powerModuleFailureCleared</uei>
            <parameter-assignment matching-group="1" parameter-name="powerSupplyIndex"/>
        </ueiMatch>

        <!-- <186>56301: RP/0/RSP0/CPU0:Jan 12 07:52:34.051 : envmon[214]: %PLATFORM-ENVMON-2-FAN_FAIL : Fan failure on slot 0/FT0/SP, fan 1, RPM 0 -->
        <ueiMatch>
            <match type="regex" expression="%PLATFORM-ENVMON-2-FAN_FAIL\s*: Fan failure on slot (\S+), fan (\d+), RPM (\d+)"/>
            <uei>uei.opennms.org/vendor/cisco/syslog/fanFailure</uei>
            <parameter-assignment matching-group="1" parameter-name="slotName"/>
            <parameter-assignment matching-group="2" parameter-name="fanIndex"/>
            <parameter-assignment matching-group="3" parameter-name="fanRpm"/>
        </ueiMatch>

    </ueiList>
</syslogd-configuration-group>
