            "type": "category",
            "vocab": [
                "<UNK>",
                "BgpPeer",
                "SnmpInterface",
                "Node",
                "SnmpInterfaceLink"
            ]
        },
        {
            "name": "same_instance",
            "tied_weights": null,
            "type": "binary"
        },
        {
            "name": "same_parent",
            "tied_weights": null,
            "type": "binary"
        },
        {
            "name": "share_ancestor",
            "tied_weights": null,
            "type": "binary"
        },
        {
            "name": "time_delta_seconds",
